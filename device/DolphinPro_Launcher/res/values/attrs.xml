<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DashedCircularProgress">
        <attr name="external_color" format="color" />
        <attr name="base_color" format="color" />
        <attr name="progress_color" format="color" />
        <attr name="max" format="float" />
        <attr name="min" format="float" />
        <attr name="progress_icon" format="reference" />
        <attr name="duration" format="integer" />
        <attr name="progress_stroke_width" format="integer" />
        
    </declare-styleable>
        <declare-styleable name="roundAngleImageView">
        <attr name="roundWidth" format="dimension" />
        <attr name="roundHeight" format="dimension" />
    </declare-styleable>
    
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
</resources>