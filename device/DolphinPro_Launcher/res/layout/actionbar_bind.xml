<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="96px" >
    
    <ImageButton
        android:id="@+id/actionbar_bind_prev"
        android:layout_width="64px"
        android:layout_height="64px" 
        android:layout_marginLeft="32px"
        android:onClick="backKey"
        android:layout_centerVertical="true"
        android:background="@drawable/back_btn_selector"/>
    
    
    <TextView
        android:id="@+id/actionbar_bind_name"
        android:layout_width="wrap_content"  
        android:layout_height="wrap_content" 
        android:layout_toRightOf="@+id/actionbar_bind_prev" 
        android:layout_marginLeft="32px"
        style="@style/h4"
        android:layout_centerVertical="true"
        android:text="@string/bind_dolphin"/>
    
    <TextView
        android:id="@+id/actionbar_dolphin_num"
        android:layout_width="wrap_content"  
        android:layout_height="wrap_content" 
        android:layout_marginRight="48px"
        android:layout_alignParentRight="true"
        style="@style/h5"
        android:layout_centerVertical="true"
        android:text="@string/dolphin_number"/>

</RelativeLayout>
